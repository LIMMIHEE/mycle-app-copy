        tools:layout_editor_absoluteY="618dp" />

    <Button
        android:id="@+id/Invitation_Event"
        android:layout_width="411dp"
        android:layout_height="wrap_content"
        android:background="#00FFFFFF"
        android:drawableStart="@drawable/present"
        android:drawablePadding="10dp"
        android:gravity="center|left"
        android:paddingLeft="20dp"
        android:text="친구 초대 이벤트"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="717dp" />

    <Button
        android:id="@+id/Propensity_test"
        android:layout_width="411dp"
        android:layout_height="wrap_content"
        android:background="#00FFFFFF"
        android:drawableStart="@drawable/mushimegane_boy"
        android:drawablePadding="10dp"
        android:gravity="center|left"
        android:paddingLeft="20dp"
        android:text="운전자 성향 테스트"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="781dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="데이터 연동 설정"
        tools:layout_editor_absoluteX="23dp"
        tools:layout_editor_absoluteY="854dp" />

    <Button
        android:id="@+id/gas_station_point"
        android:layout_width="411dp"
        android:layout_height="wrap_content"
        android:background="#00FFFFFF"
        android:drawableStart="@drawable/car_hokyuu_gasoline"
        android:drawablePadding="10dp"
        android:gravity="center|left"
        android:paddingLeft="20dp"
        android:text="주유소 포인트 적립 연동"
        tools:layout_editor_absoluteX="2dp"
        tools:layout_editor_absoluteY="889dp" />

</androidx.constraintlayout.widget.ConstraintLayout>